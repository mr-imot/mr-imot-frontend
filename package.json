{
  "name": "mrimot.com-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "pnpm run generate:slug-map && pnpm run generate:news-index",
    "build": "next build",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate:slug-map": "node scripts/generate-news-slug-map.js",
    "generate:news-index": "node scripts/generate-news-index.js",
    "dev": "pnpm run generate:slug-map && pnpm run generate:news-index && next dev",
    "lint": "eslint . --ignore-pattern .next --ignore-pattern dist --ignore-pattern public --ignore-pattern next-env.d.ts --ignore-pattern next.config.mjs --max-warnings=0",
    "start": "next start"
  },
  "packageManager": "pnpm@10.28.2",
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "next": "15.5.10",
    "esbuild": ">=0.25.0",
    "lodash": ">=4.17.23"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["sharp", "esbuild", "unrs-resolver"],
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom"
      ],
      "allowedVersions": {
        "react": "19",
        "react-dom": "19"
      }
    },
    "overrides": {
      "next": "15.5.10",
      "esbuild": ">=0.25.0",
      "lodash": ">=4.17.23"
    }
  },
  "browserslist": [
    "defaults",
    "not ie <= 11",
    "chrome >= 90",
    "safari >= 15",
    "firefox >= 90",
    "edge >= 90",
    "ios >= 15",
    "android >= 100"
  ],
